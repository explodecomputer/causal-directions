    file = {:Users/explodecomputer/Library/Application Support/Mendeley Desktop/Downloaded/Liu et al. - 2013 - Epigenome-wide association data implicate DNA methylation as an intermediary of genetic risk in rheumatoid arthritis.pdf:pdf},
    issn = {1546-1696},
